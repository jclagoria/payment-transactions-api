# Xata PostgreSQL Configuration
XATA_API_KEY=your_xata_api_key_here
XATA_USERNAME=your_xata_username_here

# Development Database
DATABASE_URL_POSTGRES_DEV=r2dbc:postgresql://your_username:your_api_key@your_host.sql.xata.sh/your_database:dev?sslmode=require

# Production Database
DATABASE_URL_POSTGRES_PROD=r2dbc:postgresql://your_username:your_api_key@your_host.sql.xata.sh/your_database:main?sslmode=require

# Redis Cloud Configuration
REDIS_HOST=your_redis_host_here
REDIS_PORT=your_redis_port_here
REDIS_USERNAME=your_redis_username_here
REDIS_PASSWORD=your_redis_password_here

# Numerator API
NUMERATOR_API_URL=http://localhost:3000

# Server Configuration
SERVER_PORT=8080
